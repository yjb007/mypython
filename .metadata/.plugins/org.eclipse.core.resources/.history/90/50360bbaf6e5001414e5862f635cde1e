from module.userinfo import userinfo

def menu():
    user = raw_input('username:')
    pwd = raw_input('password:')
    
    UserInfo = userinfo()
    res = UserInfo.CheckUserInfo(user,pwd)
    print res
    
    if user == 'yjb007' and pwd == '123456':
        return True
    else:
        return False


if __name__ == '__main__':
    menu()
    

